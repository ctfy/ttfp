<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:adsdk="http://schemas.android.com/apk/res/com.ttfp" android:id="@+id/mainLayout"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<com.ttfp.views.Game android:id="@+id/game"
		android:layout_width="fill_parent" android:layout_height="fill_parent" />
	<com.wooboo.adlib_android.WoobooAdView
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		adsdk:testing="false" adsdk:backgroundColor="#00000000"
		adsdk:textColor="#FFFFFFFF" adsdk:refreshInterval="30" />
</FrameLayout>
